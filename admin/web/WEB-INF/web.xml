<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logoutservlet</servlet-name>
        <servlet-class>com.phoenixAirline.Logoutservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>staffServlet</servlet-name>
        <servlet-class>staffweb.staffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddstaffServlet</servlet-name>
        <servlet-class>com.phoenixAirline.AddstaffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logoutservlet</servlet-name>
        <url-pattern>/Logoutservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>staffServlet</servlet-name>
        <url-pattern>/staffServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddstaffServlet</servlet-name>
        <url-pattern>/AddstaffServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>html/admin.html</welcome-file>
        <welcome-file>css/style.css</welcome-file>
        <welcome-file>js/script.js</welcome-file>
    </welcome-file-list>
</web-app>
